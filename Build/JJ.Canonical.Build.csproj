<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.250.$(BuildNum)</Version>
    <Description>JJ.Canonical contains shared data types used across JJ projects. JJ.Canonical.Build is a supporting tool for its development, designed to trigger once per solution build.</Description>
    <PackageTags></PackageTags>
    <Authors>JJ van Zon</Authors>
    <Copyright>Copyright © Jan Joost van Zon 2013 - 2025</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>    
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <IncludeSymbols>True</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="JJ.Canonical.Build.v3.ncrunchproject" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="JJ.AutoIncrementVersion" Version="1.8.1041" />
  </ItemGroup>

</Project>
